#ifndef ROVEPINMAP_H_
#define ROVEPINMAP_H_

//Mux pins
#define U3_MUX_S0 PE0
#define U3_MUX_S1 PE1
#define U4_MUX_S0 PH0
#define U4_MUX_S1 PH1
#define U5_MUX_S0 PE0
#define U5_MUX_S1 PE1

//Uarts
#define U3RX PA4
#define U3TX PA5
#define U2RX PA6
#define U2TX PA7
#define U6RX PK0
#define U6TX PK1
#define U7RX PP0
#define U7TX PP1




#endif /* ROVEPINMAP_H_ */
